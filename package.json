{
  "name": "VE-Common-App",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "app-build": "ng build",
    "app-prod-build": "ng build --prod",
    "ve-common-build": "ng run VE-Common:build -- --prod",
    "test": "ng run VE-Common-App:test && ng run VE-Common-App-e2e:e2e && ng run VE-Common:test",
    "parallel-lint": "parallelshell -w 'ng run VE-Common-App:lint' 'ng run VE-Common-App-e2e:lint' 'ng run VE-Common:lint' 'npm run stylelint'",
    "parallel-lint-autofix": "parallelshell -w 'ng run VE-Common-App:lint --fix' 'ng run VE-Common-App-e2e:lint --fix' 'ng run VE-Common:lint --fix' 'npm run stylelint -- --fix' ",
    "sequential-lint": "ng run VE-Common-App:lint && ng run VE-Common-App-e2e:lint && ng run VE-Common:lint &&npm run stylelint",
    "stylelint": "stylelint \"src/**/*.html\" \"src/**/*.scss\" \"projects/**/*.scss\" \"projects/**/*.html\" ",
    "app-documentation": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d documentation/VE-Common-App-documentation",
    "ve-common-documentation": "./node_modules/.bin/compodoc -p projects/ve-common/tsconfig.lib.json -d documentation/ve-common-documentation"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@compodoc/compodoc": "^1.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "parallelshell": "3.0.1",
    "protractor": "~5.3.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
